{
  "name": "amimet_admin",
  "version": "0.7.16",
  "license": "MIT",
  "scripts": {
    "dev": "npm run server",
    "prod": "npm run build && npm run server:prod",
    "build": "vite build",
    "server": "corenode ./server.js",
    "server:prod": "cross-env NODE_ENV=production corenode ./server.js"
  },
  "engines": {
    "node": ">= 16.4.0"
  },
  "lingui": {
    "locales": [
      "en",
      "es"
    ],
    "catalogs": [
      {
        "path": "src/locales/{locales}",
        "include": [
          "src/"
        ]
      }
    ],
    "localeDir": "src/locales/",
    "srcPathDirs": [
      "src/"
    ],
    "fallbackLocale": "en",
    "sourceLocale": "en",
    "format": "minimal"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5",
    "@corenode/utils": "^0.25.24",
    "@loadable/component": "^5.15.0",
    "@ragestudio/cloudlink": "^0.5.33",
    "antd": "^4.16.8",
    "classnames": "^2.3.1",
    "cookie_js": "^1.4.2",
    "corenode": "^0.25.26",
    "enquire-js": "^0.2.1",
    "express": "^4.17.1",
    "feather-reactjs": "^2.0.13",
    "history": "^4.10.1",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "platform": "^1.3.6",
    "qs": "^6.10.1",
    "react": "^16.14.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^16.14.0",
    "react-draggable": "^4.4.3",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.20.4",
    "react-reveal": "^1.2.2",
    "smart-circular": "^1.0.2",
    "store": "^2.0.12",
    "styled-components": "^5.3.0",
    "vite-plugin-ssr": "^0.2.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.0.2",
    "@capacitor/core": "^3.0.2",
    "@capacitor/ios": "^3.0.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/babel__core": "^7.1.14",
    "@types/node": "^14.14.36",
    "@types/react": "^16.14.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "less": "^4.1.1",
    "less-vars-to-js": "^1.3.0",
    "typescript": "^4.2.3",
    "vite": "^2.4.3",
    "vite-aliases": "^0.8.3",
    "vite-plugin-babel-import": "^2.0.5",
    "vite-plugin-commonjs-externals": "^0.1.1",
    "vite-plugin-imp": "^2.0.7"
  }
}
