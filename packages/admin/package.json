    "enquire-js": "^0.2.1",
    "evite": "^0.4.6",
    "feather-reactjs": "^2.0.13",
    "js-cookie": "^2.2.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-color": "^2.19.3",
    "react-draggable": "^4.4.3",
    "react-reveal": "^1.2.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.2.2",
    "@capacitor/core": "^3.2.2",
    "@capacitor/ios": "^3.0.2",
    "@types/babel__core": "^7.1.14",
    "@types/node": "^14.14.36"
